create_table("issues") {
	t.Column("id", "uuid", {"primary": true})
	t.Column("issue_id", "integer", {"unique": true})
	t.Column("title", "text", {"null": true})
	t.Column("experience_needed", "string", {"null": true, "default": "non-easy"})
	t.Column("url", "text", {"unique": true})
	t.Column("body", "text", {"null": true})
	t.Column("type", "string", {"null": true})
	t.Column("project_id", "uuid", {})
	t.ForeignKey("project_id", {"projects": ["id"]}, {})
	t.Column("number", "integer", {})
	t.Column("closed", "bool", {"default": false})
	t.Column("technologies", "varchar[]", {"null": true})
	t.Column("labels", "varchar[]", {"null": true})
	t.Column("is_github", "bool", {"default": false})
}
